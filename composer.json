{
  "name": "mi/wowza-guzzle-client",
  "description": "Guzzle Client for Wowza Server",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Jan Arnold",
      "email": "jan.arnold@movingimage.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~5.0",
    "guzzlehttp/psr7": "~1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.3.2"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Bundle\\WowzaGuzzleClientBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Bundle\\WowzaGuzzleClientBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0"
    }
  }
}
