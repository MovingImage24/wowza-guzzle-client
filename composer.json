{
  "name": "mi/wowza-guzzle-client",
  "description": "Guzzle Client for Wowza Server",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^6.5 || ^7.5",
    "symfony/config": "^5.2",
    "symfony/dependency-injection": "^5.2",
    "symfony/http-kernel": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Bundle\\WowzaGuzzleClientBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Bundle\\WowzaGuzzleClientBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
